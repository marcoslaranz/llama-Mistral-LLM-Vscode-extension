{
  "name": "mistral-llm-ext",
  "publisher": "marcoslara",
  "displayName": "Mistral LLM ext",
  "description": "This extension provides a command to interact with the Mistral LLM.",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mistral-ext.llm",
        "title": "Mistral LLM"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "ollama": "^0.5.16"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcoslaranz/llama-Mistral-LLM-Vscode-extension.git"
  },
  "icon": "icon.png"
}
